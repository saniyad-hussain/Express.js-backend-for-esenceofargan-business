  "main": "index.js",
  "scripts": {
    "start": "node bin/www",
    "dev": "cross-env NODE_ENV=development concurrently -k -n babel,node \"npm run babel-dev\" \"npm run server-dev\"",
    "babel-dev": "nodemon --watch src -e js,css,scss,json --exec \"babel -d build --ignore **/*.test.js\" src",
    "server-dev": "nodemon -d .25 -i src bin/www",
    "build": "cross-env NODE_ENV=production babel src -d build --ignore **/*.test.js",
    "build-dev": "cross-env NODE_ENV=development babel src -d build --ignore **/*.test.js",
    "test": "jest"
  },
