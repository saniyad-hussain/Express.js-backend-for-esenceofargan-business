    "express-promise-router": "^3.0.3",
    "express-session": "^1.15.6",
    "helmet": "^3.16.0",
    "knex": "^0.17.5",
    "postcss-import": "^12.0.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "scrypt-for-humans": "^2.0.5",
